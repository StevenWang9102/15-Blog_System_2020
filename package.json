{
  "name": "my-app",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@babel/preset-env": "^7.8.3",
    "@babel/preset-react": "^7.8.3",
    "axios": "0.19.1",
    "dateformat": "^3.0.3",
    "json-server": "^0.16.1",
    "react": "16.12.0",
    "react-dom": "16.12.0",
    "react-jss": "^10.0.4",
    "react-redux": "7.1.3",
    "react-router-dom": "5.1.2",
    "react-scripts": "3.4.0",
    "redux-saga": "1.1.3",
    "request": "^2.88.2",
    "sass": "1.3.0",
    "typescript": "3.7.4"
  },
  "scripts": {
    "start": "concurrently \"json-server --watch db.json\" \"react-scripts start\"",
    "start-json": "json-server --watch db.json",
    "build": "react-scripts build",
    "test": "jest",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": "react-app",
    "rules": {
      "jsx-a11y/anchor-is-valid": "off"
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "jest": "^24.9.0",
    "redux-devtools-extension": "^2.13.8",
    "react-test-renderer": "^16.12.0",
    "concurrently": "4.0.1"
  },
  "jest": {
    "verbose": true,
    "clearMocks": true,
    "collectCoverage": true,
    "transform": {
      "^.+\\.js$": "babel-jest"
    },
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
      "\\.(css|scss)$": "<rootDir>/__mocks__/styleMock.js"
    }
  }
}
